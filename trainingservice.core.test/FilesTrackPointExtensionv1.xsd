<script type="text/javascript">
    var pagePath         = '/products/partNum/xmlschemas/TrackPointExtension/v1/TrackPointExtensionv1.xsd';
    var pagePathSegments = pagePath.split('/');
    if (pagePath.indexOf('.ep') == -1 && pagePath.indexOf('.html') == -1 && pagePath.indexOf('.htm') == -1 && pagePath.search(/[a-z][a-z]\-[A-Z][A-Z]/) != -1) {
      pagePath = '/';
    }

    jQuery('.country-list a').click(function(e){
        e.preventDefault();
        writeCookies(jQuery(this).attr('data-locale'));
        window.location = jQuery(this).attr('href');
    });
    function writeCookies(myLocale) {
        //set cookie from my data attr
        var locale = myLocale;
        document.cookie = 'GarminUserPrefs='+locale+'; path=/; domain=.garmin.com;';
        var localeUnder = locale.replace('-','_');
        document.cookie = 'GarminLocalePref='+localeUnder+'; path=/; domain=.garmin.com;';
        garminGlobalStore.setGlobal('locale',locale);
    }
    var manualSelect = false;
    if (manualSelect == true) {
        jQuery('#loading').remove();
        jQuery('body > .container').show();
    } else {
        var goLocale   = garminGlobalStore.getGlobal('locale');
        var goLink     = jQuery('.country-list a[data-locale='+ goLocale +']');
        var userLocale = window.garminHost.env.browser.locale;
        var userLink   = jQuery('.country-list a[data-locale='+ userLocale +']');
        if (typeof(goLocale) != 'undefined' && goLocale != false && goLink.length > 0) {
           writeCookies(goLocale);
           window.location = goLink.attr('href');
        } else {
           jQuery('#loading').remove();
           jQuery('body > .container').show();
        }
    }
</script>

